<?xml version="1.0" encoding="UTF-8"?>
<settings>
	<pluginGroups>
		<pluginGroup>org.sonarsource.scanner.maven</pluginGroup>
	</pluginGroups>
	<profiles>
		<profile>
			<id>default</id>
			<repositories>
				<repository>
					<id>nexussnaps</id>
					<url>https://nexus.sbx.zone/repository/maven-public
					</url>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>
			</repositories>
			<pluginRepositories>
				<pluginRepository>
					<id>nexussnaps</id>
					<url>https://nexus.sbx.zone/repository/maven-public
					</url>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</pluginRepository>
			</pluginRepositories>
		</profile>
	</profiles>
	<servers>
		<server>
			<id>deploy-repo</id>
			<username>${nexus.user}</username>
			<password>${nexus.password}</password>
		</server>
		<server>
			<id>nexus-releases</id>
			<username>${releases.nexus.user}</username>
			<password>${releases.nexus.password}</password>
		</server>
		<server>
			<id>nexus-snapshots</id>
			<username>${snapshots.nexus.user}</username>
			<password>${snapshots.nexus.password}</password>
		</server>
	</servers>
	<mirrors>
		<mirror>
			<id>central-proxy</id>
			<mirrorOf>central</mirrorOf>
			<url>https://nexus.sbx.zone/repository/maven-public
			</url>
		</mirror>
	</mirrors>
</settings>
